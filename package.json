{
  "name": "vue-design-system",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/ui/src/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@poleski/eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "packageManager": "pnpm@7.26.3"
}
