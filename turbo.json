{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".histoire/dist/**"]
    },
    "lint": {},
    "test": {
      "inputs": ["**/*.ts", "**/*.vue"],
      "dependsOn": ["build"]
    },
    "dev": {
      "cache": false
    }
  }
}
