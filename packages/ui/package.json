{
  "name": "ui",
  "version": "0.0.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue --fix",
    "prebuild": "rimraf ./dist",
    "build": "vite build && pnpm run build:types",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "storybook": "export SET NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006",
    "build-storybook": "export SET NODE_OPTIONS=--openssl-legacy-provider && build-storybook"
  },
  "dependencies": {
    "cva": "npm:class-variance-authority@^0.4.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@iconify/vue": "^4.1.0",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/vue3": "^6.5.16",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.0.2",
    "storybook-dark-mode": "^2.0.6",
    "tailwindcss": "^3.2.7",
    "tsconfig": "workspace:*",
    "typescript": "^4.9.5",
    "vite": "^4.1.2",
    "vue-loader": "^16.8.3"
  }
}
